filetype plugin on
filetype indent on  

set incsearch
set ignorecase
set showmatch
set hlsearch

set showcmd
set showmode
set wildmenu
set wildmode=list:longest

set number
set cursorline
set textwidth=72
set tabstop=4
set shiftwidth=4
set expandtab
syntax on

set background=light
let g:gruvbox_contrast_light = 'high'
colorscheme gruvbox
