filetype plugin on
filetype indent on  
set omnifunc=syntaxcomplete#Complete

set noswapfile
set incsearch
set ignorecase
set smartcase
set showmatch
set hlsearch

set scrolloff=5

set showcmd
set showmode
set wildmenu
set wildmode=list:longest

set number
set cursorline
set tabstop=4
set shiftwidth=4
set expandtab
set autoindent
syntax on

set laststatus=2
set statusline+=%f
set background=light
let g:gruvbox_contrast_light = 'high'
colorscheme gruvbox
